<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="58.0" prefWidth="944.0" spacing="7.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.PanelGornyController">
   <children>
      <ToggleButton fx:id="tableButton" mnemonicParsing="false" onAction="#launchTable" prefHeight="56.0" prefWidth="147.0" selected="true" text="Tabela">
         <toggleGroup>
            <ToggleGroup fx:id="grupaPrzyciskow" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton fx:id="productsListButton" mnemonicParsing="false" onAction="#launchProductList" prefHeight="56.0" prefWidth="147.0" text="Lista produktów" toggleGroup="$grupaPrzyciskow" />
      <ToggleButton fx:id="weightButton" mnemonicParsing="false" prefHeight="48.0" prefWidth="76.0" text="Waga" toggleGroup="$grupaPrzyciskow" />
      <Label prefHeight="38.0" prefWidth="483.0" text="ARKUSZ DO LICZENIA KALORII ORAZ BIAŁKA" textFill="#005eff">
         <HBox.margin>
            <Insets bottom="5.0" left="35.0" right="5.0" top="5.0" />
         </HBox.margin>
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
